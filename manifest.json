{
  "manifest_version": 2,
  "name": "Alarm",
  "version": "1.1",
  "description": "OSD feature 2",

  "icons": {
    "64": "logo_64.png"
  },

  "permissions": [
    "alarms",
    "tabs",
    "notifications",
    "storage"
  ],

  "browser_action": {
    "default_icon": "logo_32.png",
    "default_title": "Alarm",
    "default_popup": "index.html"
  },
  "background": {
      "scripts": ["background.js"]
  }

}
